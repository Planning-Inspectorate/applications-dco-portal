{
	"compilerOptions": {
		"types": ["cypress", "node"],
		"allowImportingTsExtensions": true,
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"baseUrl": ".",
		"checkJs": false,
		"erasableSyntaxOnly": true,
		"esModuleInterop": true,
		"module": "NodeNext",
		"moduleResolution": "nodenext",
		"noEmit": true,
		"strict": true,
		"target": "esnext",
		"skipLibCheck": true
	},
	"include": ["**/*.ts"],
	"exclude": ["node_modules"]
}
